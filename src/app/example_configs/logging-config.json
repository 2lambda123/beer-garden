{
    "disable_existing_loggers": false, 
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    }, 
    "handlers": {
        "beer_garden": {
            "class": "logging.StreamHandler", 
            "formatter": "simple", 
            "level": "INFO", 
            "stream": "ext://sys.stdout"
        }
    }, 
    "loggers": {
        "pika": {
            "level": "ERROR"
        },
        "requests.packages.urllib3.connectionpool": {
            "level": "WARN"
        },
        "tornado.access": {
            "level": "WARN"
        },
        "tornado.application": {
            "level": "WARN"
        },
        "tornado.general": {
            "level": "WARN"
        }
    }, 
    "root": {
        "handlers": [
            "beer_garden"
        ], 
        "level": "INFO"
    }, 
    "version": 1
}